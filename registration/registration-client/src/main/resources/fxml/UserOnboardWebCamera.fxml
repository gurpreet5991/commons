<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.collections.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.stage.Screen?>
<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:id="faceCapture" styleClass="onboardParenPane" fx:controller="io.mosip.registration.controller.device.FaceCaptureController">
   <children>
      <AnchorPane layoutX="1.0" styleClass="userOnboardHeader">
         <children>
            <Text fx:id="onboardHomeLink" layoutX="20.0" layoutY="20.0" onMouseClicked="#goToHomePage" text="%homeLink" />
            <Text fx:id="onboardPageDesc" layoutX="68.0" layoutY="20.0" text="%onboardOperator" />
            <Label fx:id="onboardPageTitle" layoutX="559.0" layoutY="2.0" text="%onboardOperator" />
         </children>
      </AnchorPane>
      <AnchorPane layoutX="1.0" layoutY="33.0" styleClass="userOnboardBackPane">
         <children>
            <AnchorPane layoutX="1130.0" layoutY="152.0" prefHeight="200.0" prefWidth="141.0">
               <children>
                  <ImageView fx:id="userOnboardImg" fitHeight="192.0" fitWidth="130.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/userOnboardFaceTracker.png" />
                     </image>
                  </ImageView>
                  <Label layoutX="36.0" layoutY="0.0" styleClass="completedTracker" text="Exception" />
                  <Label layoutX="36.0" layoutY="67.0" styleClass="completedTracker" text="Fingerprints" />
                  <Label layoutX="36.0" layoutY="122.0" styleClass="completedTracker" text="Iris" />
                  <Label layoutX="36.0" layoutY="177.0" styleClass="currentTracker" text="Photo" />
               </children>
            </AnchorPane>
            <AnchorPane layoutX="180.0" styleClass="onboardImgPane">
               <children>
                  <AnchorPane layoutX="357.0" layoutY="254.0" styleClass="onboardFaceDetails">
                     <children>
                        <Label layoutX="92.0" layoutY="6.0" styleClass="onboardBioDetailsHeader" text="%image" />
                        <Label layoutY="35.0" styleClass="onboardBioDetails" text="%threshold" />
                        <Label layoutX="150.0" layoutY="35.0" styleClass="onboardBioDetails" text="%quality" />
                        <Label layoutY="70.0" styleClass="onboardBioDetails" text="80%" />
                        <Label layoutX="150.0" layoutY="70.0" styleClass="onboardBioDetails" text="-" />
                        <Label layoutY="100.0" styleClass="onboardBioDetails" text="%attempts" />
                        <Label layoutX="150.0" layoutY="100.0" styleClass="onboardBioDetails" text="%quality" />
                        <Label layoutX="150.0" layoutY="125.0" styleClass="onboardBioDetails" text="-" />
                        <Label layoutY="125.0" styleClass="onboardBioDetails" text="-" />
                     </children>
                  </AnchorPane>
                  <Label layoutX="248.0" layoutY="30.0" styleClass="onboardPageBioTitle" text="%image" />
                  <AnchorPane layoutX="100.0" layoutY="415.0" styleClass="onboardProgressPane">
                     <children>
                        <Button fx:id="takePhoto" layoutX="620.0" layoutY="65.0" mnemonicParsing="false" onAction="#takePhoto" styleClass="onboardScanBtn" text="%takePhoto">
                           <graphic>
                              <ImageView fitHeight="15.0" fitWidth="15.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../images/scan.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                        <Label layoutX="151.0" layoutY="71.0" text="%quality" />
                        <Line endX="100.0" layoutX="372.0" layoutY="85.0" startX="-100.0" />
                     </children>
                  </AnchorPane>
                  <AnchorPane id="applicantPhoto" fx:id="applicantImagePane" layoutX="352.0" layoutY="47.0" onMouseClicked="#enableCapture" prefHeight="214.0" prefWidth="242.0">
                     <children>
                        <ImageView fx:id="applicantImage" fitHeight="210.0" fitWidth="240.0" layoutX="2.0" layoutY="2.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/Photo.png" />
                           </image>
                        </ImageView>
                     </children>
                  </AnchorPane>
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
      <AnchorPane layoutX="1.0" layoutY="620.0" styleClass="userOnboardFooter">
         <children>
            <Button fx:id="biometricPrevBtn" layoutX="124.0" layoutY="3.0" mnemonicParsing="false" onAction="#goToPreviousPane" styleClass="backButton" text="%back">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="27.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/arrowLeft.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="saveBiometricDetailsBtn" layoutX="991.0" layoutY="4.0" mnemonicParsing="false" onAction="#saveBiometricDetails" styleClass="contiuneButton" text="%continue">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="27.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/arrowRight.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </AnchorPane>
      <Label fx:id="photoAlert" layoutX="894.0" layoutY="540.0" prefHeight="27.0" prefWidth="194.0" />
   </children>
</AnchorPane>