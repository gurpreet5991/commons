      		<TableView fx:id="table" layoutX="10.0" layoutY="73.0" prefHeight="278.0" prefWidth="625.0">
      			<columns>
      				<TableColumn fx:id="id" prefWidth="238.0" resizable="false" text="Registration ID" />
      				<TableColumn fx:id="type" prefWidth="40.0" resizable="false" text="Type" />
      				<TableColumn fx:id="residentName" prefWidth="120.0" resizable="false" text="Resident Name" />
      				<TableColumn fx:id="operatorId" minWidth="0.0" prefWidth="100.0" resizable="false" text="Operator ID" />
      				<TableColumn fx:id="operatorName" minWidth="0.0" prefWidth="125.0" resizable="false" text="Operator Name" />
                  <TableColumn fx:id="acknowledgementFormPath" minWidth="0.0" prefWidth="0.0" resizable="false" text="Operator Name" visible="false" />
      			</columns>
      		</TableView>
            <AnchorPane layoutX="635.0" layoutY="34.0" prefHeight="390.0" prefWidth="374.0">
               <children>
                  <ImageView fx:id="imageView" fitHeight="379.0" fitWidth="361.0" layoutX="4.0" layoutY="3.0" onMouseClicked="#openAckForm" pickOnBounds="true" preserveRatio="true" />
               </children>
            </AnchorPane>
            <Button fx:id="approvalBtn" layoutX="383.0" layoutY="34.0" mnemonicParsing="false" onAction="#approvePacket" onMouseClicked="#goToHomePage" prefHeight="20.0" prefWidth="80.0" text="Approve">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="rejectionBtn" layoutX="469.0" layoutY="34.0" mnemonicParsing="false" onAction="#rejectPacket" prefHeight="20.0" prefWidth="80.0" text="Reject" textAlignment="CENTER">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="onHoldBtn" layoutX="555.0" layoutY="34.0" mnemonicParsing="false" onAction="#onHoldPacket" prefHeight="20.0" prefWidth="80.0" text="On Hold" textAlignment="CENTER">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
      <ImageView fitHeight="20.0" fitWidth="20.0" layoutX="20.0" layoutY="18.0" onMouseClicked="#goToHomePage" pickOnBounds="true" preserveRatio="true">
