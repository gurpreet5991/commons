<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="480.0" prefWidth="777.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.mosip.registration.controller.device.IrisCaptureController">
   <children>
      <Label layoutX="29.0" layoutY="27.0" text="Iris Scan" />
      <Pane id="leftIrisPane" fx:id="leftIrisPane" layoutX="29.0" layoutY="68.0" prefHeight="165.0" prefWidth="235.0" styleClass="irisImagePane">
         <children>
            <ImageView fx:id="leftIrisImage" fitHeight="165.0" fitWidth="235.0" layoutX="1.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/leftIris.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <Label layoutX="29.0" layoutY="240.0" prefHeight="27.0" prefWidth="99.0" text="Quality %" />
      <Label layoutX="144.0" layoutY="240.0" text=":" />
      <Label fx:id="leftIrisQualityScore" layoutX="156.0" layoutY="240.0" />
      <Label layoutX="29.0" layoutY="280.0" text="Threshold %" />
      <Label layoutX="146.0" layoutY="280.0" text=":" />
      <Label fx:id="leftIrisThreshold" layoutX="155.0" layoutY="280.0" />
      <Pane id="rightIrisPane" fx:id="rightIrisPane" layoutX="378.0" layoutY="68.0" prefHeight="165.0" prefWidth="235.0" styleClass="irisImagePane">
         <children>
            <ImageView fx:id="rightIrisImage" fitHeight="165.0" fitWidth="235.0" layoutX="1.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/rightIris.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <Label layoutX="378.0" layoutY="240.0" prefHeight="27.0" prefWidth="99.0" text="Quality %" />
      <Label layoutX="493.0" layoutY="240.0" text=":" />
      <Label fx:id="rightIrisQualityScore" layoutX="505.0" layoutY="240.0" />
      <Label layoutX="378.0" layoutY="280.0" text="Threshold %" />
      <Label layoutX="495.0" layoutY="280.0" text=":" />
      <Label fx:id="rightIrisThreshold" layoutX="504.0" layoutY="280.0" />
      <Button fx:id="scanIris" layoutX="29.0" layoutY="341.0" mnemonicParsing="false" onAction="#scan" text="Scan" />
      <Button layoutX="701.0" layoutY="427.0" mnemonicParsing="false" onAction="#nextSection" text="Next" />
      <Button layoutX="586.0" layoutY="427.0" mnemonicParsing="false" onAction="#previousSection" text="Previous" />
   </children>
</AnchorPane>
