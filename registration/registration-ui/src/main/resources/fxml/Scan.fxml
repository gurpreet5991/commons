<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.mosip.registration.controller.device.ScanPopUpViewController" styleClass="paneRoot">
   <children>
      <AnchorPane layoutX="3.0" layoutY="53.0" styleClass="subPane">
         <children>
            <Button layoutX="387.0" layoutY="233.0" mnemonicParsing="false" onAction="#scan" text="Scan" styleClass="RegbuttonStyle" >
               <font>
                  <Font size="10.0" />
               </font></Button>
            <AnchorPane layoutX="10.0" layoutY="25.0" styleClass="imagePane">
               <children>
                  <ImageView fx:id="scanImage" fitHeight="238.0" fitWidth="349.0" layoutX="-2.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true" styleClass="scannedImagePreview" />
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
      <AnchorPane layoutY="-1.0" styleClass="headerPane">
         <children>
            <Label fx:id="popupTitle" layoutX="194.0" layoutY="14.0" styleClass="scanPopupHeader" />
            <Hyperlink layoutX="458.0" layoutY="10.0" onAction="#exitWindow" text="X" styleClass="exit"/>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
