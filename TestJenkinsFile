node{
def branch=env.NEXT_BRANCH_NAME
    stage('checkout') {
//   input 'Do you want to promote the code to QA?'
  dir('branch') {
   checkout([$class: 'GitSCM',
    branches: [
     [name: branch]
    ],
    extensions: scm.extensions + [[$class: 'CleanCheckout']],
    userRemoteConfigs: [
     [url: 'https://github.com/mosip/mosip.git', credentialsId: 'tarun']
    ]
   ])
  }
 }
}
