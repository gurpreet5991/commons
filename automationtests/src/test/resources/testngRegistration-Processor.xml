<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Mosip API Suite">
  
  
<!--   Passing Group as parmeter unsing "testType"
  Possible Groups are:
  						smoke
  						regression
  						smokeAndRegression -->
  						
   <test name="RegistrationProcessor.PacketStatus">    						
   <parameter name="testType" value="smokeAndregression"/>
    <classes>
     <class name="io.mosip.regProc.tests.PacketStatus"/>          
    </classes>
  </test> <!-- Test --> 
   <test name="RegistrationProcessor.Sync">    						
   <parameter name="testType" value="smokeAndregression"/>
    <classes>
     <class name="io.mosip.regProc.tests.Sync"/>          
    </classes>
  </test>
  <test name="RegistrationProcessor.PacketReceiver">    						
   <parameter name="testType" value="smokeAndregression"/>
    <classes>
     <class name="io.mosip.regProc.tests.PacketReceiver"/>          
    </classes>
  </test>
   <test name="RegistrationProcessor.IntegrationScenarios">    						
    <classes>
     <class name="io.mosip.regProc.tests.IntegrationScenarios"/>          
    </classes>
  </test> 
</suite> <!-- Suite -->
